<?xml version="1.0" encoding="utf-8"?>
<com.jbak.superbrowser.ui.LinearLayoutEx xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <TextView android:id="@+id/title"
        android:maxLines="4"
        style="@style/dialogTitle"
        />
    <FrameLayout 
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/contentFrame"
        android:padding="@dimen/magic_padding"
        />
	<com.jbak.superbrowser.ui.HorizontalPanel 
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:id="@+id/bottomPanel"
	    />
</com.jbak.superbrowser.ui.LinearLayoutEx>
